{
  "dns": {
    "servers": [
      {
        "tag": "local",
        "type": "udp",
        "server": "1.1.1.1"
      },
      {
        "tag": "public",
        "type": "https",
        "server": "dns.google",
        "domain_resolver": "local"
      },
      {
        "tag": "foreign",
        "type": "https",
        "server": "8.8.8.8",
        "detour": "默认节点"
      },
      {
        "tag": "fakeip",
        "type": "fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      }
    ],
    "rules": [
      {
        "clash_mode": "direct",
        "server": "local"
      },
      {
        "clash_mode": "global",
        "server": "fakeip"
      },
      {
        "query_type": "HTTPS",
        "action": "reject"
      },
      {
        "domain_suffix": [
          "gov.my",
          "edu.my",
          "my"
        ],
        "server": "local"
      },
      {
        "rule_set": [
          "geoip-my"
        ],
        "server": "local"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "fakeip",
        "rewrite_ttl": 1
      }
    ],
    "final": "foreign",
    "strategy": "ipv4_only",
    "independent_cache": true,
    "reverse_mapping": true
  },
  "outbounds": [
    {
      "tag": "默认节点",
      "type": "selector",
      "outbounds": [
        "HK手动",
        "SG手动"
      ]
    },
    {
      "tag": "OpenAI",
      "type": "selector",
      "outbounds": [
        "直连",
        "HK手动",
        "SG手动",
        "REJECT"
      ]
    },
    {
      "tag": "Google",
      "type": "selector",
      "outbounds": [
        "直连",
        "HK手动",
        "SG手动",
        "REJECT"
      ]
    },
    {
      "tag": "Github",
      "type": "selector",
      "outbounds": [
        "直连",
        "HK手动",
        "SG手动",
        "REJECT"
      ]
    },
    {
      "tag": "Telegram",
      "type": "selector",
      "outbounds": [
        "直连",
        "HK手动",
        "SG手动",
        "REJECT"
      ]
    },
    {
      "tag": "测速工具",
      "type": "selector",
      "outbounds": [
        "直连",
        "HK手动",
        "SG手动",
        "REJECT"
      ]
    },
    {
      "tag": "抖音",
      "type": "selector",
      "outbounds": [
        "直连",
        "HK手动",
        "SG手动",
        "REJECT"
      ]
    },
    {
      "tag": "HK手动",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🇭🇰|HK|hk|香港|港|HongKong"
          ]
        },
        {
          "action": "exclude",
          "keywords": [
            "免费"
          ]
        }
      ]
    },
    {
      "tag": "SG手动",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "新加坡|坡|狮城|SG|Singapore"
          ]
        },
        {
          "action": "exclude",
          "keywords": [
            "免费"
          ]
        }
      ]
    },
    {
      "tag": "最终",
      "type": "selector",
      "outbounds": [
        "默认节点",
        "直连",
        "REJECT"
      ]
    },
    {
      "tag": "GLOBAL",
      "type": "selector",
      "outbounds": [
        "默认节点",
        "Github",
        "OpenAI",
        "Google",
        "Telegram",
        "测速工具",
        "抖音",
        "HK手动",
        "SG手动",
        "REJECT",
        "直连"
      ]
    },
    {
      "tag": "直连",
      "type": "direct"
    },
    {
      "tag": "REJECT",
      "type": "block"
    }
  ],
  "route": {
    "rules": [
      {
        "action": "sniff",
        "sniffer": [
          "http",
          "tls",
          "quic",
          "dns"
        ]
      },
      {
        "inbound": "dns-in",
        "action": "hijack-dns"
      },
      {
        "ip_is_private": true,
        "outbound": "直连"
      },
      {
        "clash_mode": "direct",
        "outbound": "直连"
      },
      {
        "clash_mode": "global",
        "outbound": "GLOBAL"
      },
      {
        "rule_set": "geosite-openai",
        "outbound": "OpenAI"
      },
      {
        "rule_set": "geosite-github",
        "outbound": "Github"
      },
      {
        "rule_set": "geosite-telegram",
        "outbound": "Telegram"
      },
      {
        "rule_set": "geoip-telegram",
        "outbound": "Telegram"
      },
      {
        "rule_set": "geosite-google",
        "outbound": "Google"
      },
      {
        "rule_set": "geosite-speedtest-tools",
        "outbound": "测速工具"
      },
      {
        "rule_set": "geosite-douyin",
        "outbound": "抖音"
      }
    ],
    "rule_set": [
      {
        "tag": "geosite-openai",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo/geosite/openai.srs",
        "download_detour": "默认节点"
      },
      {
        "tag": "geoip-my",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo/geoip/my.srs",
        "download_detour": "默认节点"
      },
      {
        "tag": "geosite-github",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo/geosite/github.srs",
        "download_detour": "默认节点"
      },
      {
        "tag": "geosite-telegram",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo/geosite/telegram.srs",
        "download_detour": "默认节点"
      },
      {
        "tag": "geoip-telegram",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo/geoip/telegram.srs",
        "download_detour": "默认节点"
      },
      {
        "tag": "geosite-google",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/MetaCubeX/meta-rules-dat/refs/heads/sing/geo/geosite/google.srs",
        "download_detour": "默认节点"
      },
      {
        "tag": "geosite-speedtest-tools",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/WizisCool/proxy-rules/refs/heads/main/geo/geosite/speedtest-tools.srs",
        "download_detour": "默认节点"
      },
      {
        "tag": "geosite-douyin",
        "type": "remote",
        "format": "binary",
        "url": "https://raw.githubusercontent.com/WizisCool/proxy-rules/refs/heads/main/geo/geosite/douyin.srs",
        "download_detour": "默认节点"
      }
    ],
    "final": "最终",
    "default_domain_resolver": {
      "server": "public"
    }
  },
  "inbounds": [
    {
      "tag": "dns-in",
      "type": "direct",
      "listen": "::",
      "listen_port": 1053
    },
    {
      "tag": "http-in",
      "type": "http",
      "listen": "::",
      "listen_port": 8080
    },
    {
      "tag": "socks-in",
      "type": "socks",
      "listen": "::",
      "listen_port": 1080
    },
    {
      "tag": "redirect-in",
      "type": "redirect",
      "listen": "::",
      "listen_port": 7890
    },
    {
      "tag": "tproxy-in",
      "type": "tproxy",
      "listen": "::",
      "listen_port": 7891
    },
    {
      "tag": "tun-in",
      "type": "tun",
      "interface_name": "momo",
      "address": [
        "172.31.0.1/30",
        "fdfe:dcba:9876::1/126"
      ],
      "auto_route": false,
      "auto_redirect": false
    }
  ],
  "experimental": {
    "cache_file": {
      "enabled": true,
      "path": "/etc/momo/run/cache.db",
      "store_fakeip": true
    },
    "clash_api": {
      "external_controller": "0.0.0.0:9095",
      "external_ui": "/etc/momo/run/ui",
      "external_ui_download_url": "https://gh-proxy.com/https://github.com/Zephyruso/zashboard/archive/refs/heads/gh-pages.zip",
      "external_ui_download_detour": "直连",
      "secret": "",
      "default_mode": "rule"
    }
  },
  "log": {
    "disabled": false,
    "level": "info",
    "timestamp": true
  }
}